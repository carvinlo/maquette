{
  "compilerOptions": {
    "lib": [
      "dom",
      "es2015",
      "es6"
    ],
    "module": "es6",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": ".",
    "rootDir": ".",
    "target": "es5",
    "noEmitOnError": false,
    "strictNullChecks": true,
    "noImplicitThis": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.d.ts"
  ],
  "exclude": []
}
